{
  "name": "gamedaim-server",
  "version": "0.0.1",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --exit-child --respawn src/app.ts",
    "start": "node dist/src/app.js",
    "build": "rimraf ./dist && tsc",
    "clean": "rimraf node_modules build dist",
    "clean:build": "rimraf build dist",
    "lint": "eslint .",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "tap --node-arg=--require=ts-node/register/transpile-only --no-coverage",
    "db:generate": "pnpx prisma generate",
    "db:migrate-dev": "pnpx prisma migrate dev --name init",
    "db:migrate-deploy": "npm run prisma migrate deploy",
    "db:seed": "pnpx prisma db seed",
    "db:push": "pnpx prisma db push --skip-generate",
    "db:studio": "pnpx prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.329.0",
    "@aws-sdk/lib-storage": "^3.329.0",
    "@faker-js/faker": "^7.6.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.1",
    "@prisma/client": "^4.13.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "eslint": "8.40.0",
    "fastify": "^4.17.0",
    "fastify-zod": "^1.3.2",
    "nanoid": "3.3.6",
    "pino-pretty": "^10.0.0",
    "sharp": "^0.32.1",
    "tap": "^16.3.4",
    "ts-mock-imports": "^1.3.8",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.1.1",
    "@types/tap": "^15.0.8",
    "eslint-config-node-custom": "workspace:*",
    "sinon": "^15.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "typescript-config": "workspace:*"
  }
}
