{
  "name": "gamedaim-server",
  "version": "0.0.1",
  "main": "./dist/app.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "dev": "tsup --watch --onSuccess 'node ./dist/app.js'",
    "build": "rimraf ./dist && tsup",
    "prebuild": "npm run lint",
    "clean": "rimraf node_modules build dist",
    "clean:build": "rimraf build dist .nyc_output",
    "lint": "eslint .",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "tap --node-arg=--require=ts-node/register/transpile-only --no-coverage",
    "db:generate": "pnpx prisma generate",
    "db:migrate-dev": "pnpx prisma migrate dev --name init",
    "db:migrate-deploy": "npm run prisma migrate deploy",
    "db:seed": "pnpx prisma db seed",
    "db:push": "pnpx prisma db push --skip-generate",
    "db:studio": "pnpx prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.342.0",
    "@aws-sdk/lib-storage": "^3.342.0",
    "@faker-js/faker": "^8.0.2",
    "@fastify/cors": "^8.3.0",
    "@fastify/jwt": "^7.0.0",
    "@fastify/multipart": "^7.6.0",
    "@fastify/swagger": "^8.5.1",
    "@fastify/swagger-ui": "^1.8.1",
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "digiflazz-sdk": "^0.0.2",
    "dotenv": "^16.1.1",
    "eslint": "8.41.0",
    "fastify": "^4.17.0",
    "fastify-zod": "^1.3.2",
    "nanoid": "3.3.6",
    "pino-pretty": "^10.0.0",
    "sharp": "^0.32.1",
    "tap": "^16.3.4",
    "tripay-sdk": "^0.0.3",
    "tslib": "^2.5.2",
    "tsup": "^6.7.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.2.5",
    "@types/tap": "^15.0.8",
    "eslint-config-node-custom": "workspace:*",
    "typescript": "^5.0.4",
    "typescript-config": "workspace:*"
  }
}
